<UserControl x:Class="FDT.Gui.CustomFields.InputFieldRiskFloodMap"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FDT.Gui.CustomFields"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="800">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/UserControls/StyleDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <StackPanel Orientation="Horizontal" Margin="{StaticResource TopDownMargin}">
        <Label Content="Flood map" Width="{StaticResource LabelLength}" Margin="{StaticResource RightMargin}"/>
        <TextBox Width="{StaticResource FilePathLength}" Margin="{StaticResource RightMargin}"/>
        <Button Content="..." Width="{StaticResource BrowseButtonLength}" Margin="{StaticResource RightMargin}"/>
        <Label Content="Return Period" Width="{StaticResource LabelLength}" Margin="{StaticResource RightMargin}"/>
        <TextBox Width="{StaticResource ReturnPeriodLength}"/>
    </StackPanel>

</UserControl>
